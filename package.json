		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"express": "^5.1.0",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1"
