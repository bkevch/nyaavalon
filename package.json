{
	"name": "nyaavalon",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm-run-all --parallel dev:sveltekit dev:socket",
		"dev:sveltekit": "vite dev",
  	"dev:socket": "node socket-server.js",
		"build": "vite build",
